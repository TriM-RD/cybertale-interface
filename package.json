{
  "name": "geocms-interface",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --config vite.config.dev.js",
    "do-not-use-build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build": "vite build --mode production",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "copy-package": "node node_modules/@cybertale/form/copy-package.mjs"
  },
  "dependencies": {
    "@campsabout/mapbox": "git+https://github.com/TriM-RD/campsabout-mapbox.git",
    "@cybertale/form": "^2.0.5",
    "@cybertale/interface": "^2.1.2",
    "@fancyapps/ui": "^5.0.36",
    "@fortawesome/fontawesome-free": "^6.6.0",
    "@geocms/localization": "git+https://github.com/TriM-RD/geocms-localization.git",
    "@popperjs/core": "^2.11.8",
    "@primevue/themes": "^4.0.4",
    "@turf/turf": "^7.1.0",
    "@uppy/core": "^3.1.2",
    "@uppy/dashboard": "^3.3.2",
    "@uppy/tus": "^3.0.6",
    "@vuepic/vue-datepicker": "^10.0.0",
    "aframe": "^1.6.0",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "fancybox": "^3.0.1",
    "mapbox-gl": "^3.8.0",
    "pinia": "^2.1.7",
    "primevue": "^4.0.4",
    "prompt-sync": "^4.2.0",
    "uuid": "^10.0.0",
    "vue": "^3.4.30",
    "vue-loading-overlay": "^6.0.6",
    "vue-recaptcha": "^3.0.0-alpha.6",
    "vue-recaptcha-v3": "^2.0.1",
    "vue-router": "^4.4.0",
    "vue-toastification": "^2.0.0-rc.5",
    "vue3-blocks-tree": "^0.6.2",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/bootstrap": "^5.2.10",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.5",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "cypress": "^13.12.0",
    "eslint": "^8.56.0",
    "eslint-plugin-cypress": "^3.3.0",
    "eslint-plugin-vue": "^9.23.0",
    "jsdom": "^24.1.0",
    "npm-run-all2": "^6.2.0",
    "prettier": "^3.2.5",
    "start-server-and-test": "^2.0.4",
    "terser": "^5.33.0",
    "typescript": "~5.4.0",
    "vite": "^5.3.1",
    "vite-plugin-checker": "^0.7.2",
    "vite-plugin-gltf": "^4.0.0",
    "vite-plugin-vue-devtools": "^7.3.1",
    "vitest": "^1.6.0",
